{
    "manifest_version": 2,

    "name": "RequestHelper",
    "description": "asdf",
    "version": "1.0",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["jquery-2.1.4.min.js","tab-handler.js"]
        }
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "webRequest",
        "activeTab",
        "*://*/*"
    ]
}